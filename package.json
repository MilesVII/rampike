{
  "name": "rampike",
  "version": "0.0.12",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "main": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
